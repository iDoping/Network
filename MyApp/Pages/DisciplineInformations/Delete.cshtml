@page
@model MyApp.Pages.DisciplineInformations.DeleteModel

@{
    ViewData["Title"] = "Delete";
}

<h2>Удалить</h2>

<h3>Вы действительно хотите удалить этот элемент?</h3>
<div>
    <h4>Информация об учебных дисциплинах</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            <p>Специальность:</p>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DisciplineInformation.Speciality)
        </dd>
        <dt>
            <p>Дисциплина:</p>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DisciplineInformation.Discipline)
        </dd>
        <dt>
            <p>ЛК:</p>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DisciplineInformation.AmountOfLections)
        </dd>
        <dt>
            <p>ЛБ:</p>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DisciplineInformation.AmountOfLabs)
        </dd>
        <dt>
            <p>ПР:</p>
        </dt>
        <dd>
            @Html.DisplayFor(model => model.DisciplineInformation.AmountOfPractics)
        </dd>
    </dl>
    
    <form method="post">
        <input type="hidden" asp-for="DisciplineInformation.ID" />
        <input type="submit" value="Удалить" class="btn btn-default" /> |
        <a asp-page="./Index">Назад</a>
    </form>
</div>
